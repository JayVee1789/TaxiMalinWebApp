<section class="contact-form-style-one">
    <div class="container">
        <div class="block-title text-center">
            <div class="dot-line"></div><!-- /.dot-line -->
            <p>Laisser un message</p>
            <h2>Objet perdu</h2>
        </div><!-- /.block-title -->
        <form asp-controller="Home" asp-action="Contact" class="contact-form-one row" id="contact-form" method="post">
            <div class="col-lg-6">
                <div class="input-holder">
                    <input asp-for="Nom" type="text" placeholder="Votre nom">
                </div><!-- /.input-holder -->
            </div><!-- /.col-lg-6 -->
            <div class="col-lg-6">
                <div class="input-holder">
                    <input asp-for="Email" type="email" placeholder="Courriel">
                </div><!-- /.input-holder -->
            </div><!-- /.col-lg-6 -->
            <div class="col-lg-6">
                <div class="input-holder">
                    <input asp-for="NumeroTelephone" type="text" placeholder="Telephone">
                </div><!-- /.input-holder -->
            </div><!-- /.col-lg-6 -->
            <div class="col-lg-6">
                <div class="input-holder">
                    <input asp-for="Sujet" type="text" placeholder="Sujet:indiquer objet perdu">
                </div><!-- /.input-holder -->
            </div><!-- /.col-lg-6 -->
            <div class="col-lg-12">
                <div class="input-holder">
                    <textarea asp-for="Message" class="form-control" name="form_message" placeholder="Ecrire votre message"></textarea>
                </div><!-- /.input-holder -->
            </div><!-- /.col-lg-6 -->
            <div class="col-lg-12">
                <div class="input-holder text-center">
                    <input id="form_botcheck" name="form_botcheck" class="form-control" type="hidden" value="">
                    <button class="theme-btn btn-style-two" type="submit" data-loading-text="Veuillez patienter..."><span>Envoyer message</span></button>
                </div><!-- /.input-holder -->
            </div><!-- /.col-lg-6 -->
        </form><!-- /.contact-form-one -->
    </div><!-- /.container -->
</section><!-- /.contact-form-style-one -->
